{
  "name": "peeky-mono-repo",
  "version": "0.9.1",
  "private": true,
  "repository": {
    "url": "https://github.com/Akryum/peeky.git",
    "type": "git"
  },
  "scripts": {
    "lint": "eslint . --ext js,vue,ts",
    "build": "pnpm run -r build --filter ./packages",
    "watch": "pnpm run build && pnpm run -r watch --parallel --filter ./packages",
    "test": "pnpm run -r test --parallel --filter ./packages",
    "release": "pnpm run lint && pnpm run build && pnpm run test && echo \"TODO publish\"",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs"
  },
  "engines": {
    "node": ">14.5.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "@vue/eslint-config-standard": "^5.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "conventional-changelog-cli": "^2.1.1",
    "eslint": "^7.18.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-vue": "^7.4.1",
    "postcss": "^8.2.4",
    "postcss-nested": "^5.0.3",
    "rimraf": "^3.0.2",
    "tailwindcss": "^2.0.2",
    "vitepress": "^0.20.4"
  }
}
