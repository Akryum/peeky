    "diffable-html": "^5.0.0",
    "expect": "^29.0.3",
    "fast-glob": "^3.2.12",
    "fs-extra": "^10.1.0",
    "fs-monkey": "^1.0.3",
    "happy-dom": "^6.0.4",
    "jest-diff": "^29.0.3",
    "lodash": "^4.17.21",
    "memfs": "^3.4.7",
    "mlly": "^0.5.16",
    "nanoid": "^4.0.0",
    "pathe": "^0.3.8",
