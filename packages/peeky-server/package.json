{
  "name": "@peeky/server",
  "version": "0.13.6",
  "description": "Server for Peeky, the testing framework for curious minds",
  "license": "MIT",
  "author": {
    "name": "Guillaume Chau"
  },
  "repository": {
    "url": "https://github.com/Akryum/peeky.git",
    "type": "git",
    "directory": "packages/peeky-server"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": "./*"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "pnpm run gen && rimraf dist && tsc -d && cp src/context.d.ts dist/context.d.ts",
    "gen": "rimraf dist src/generated && tsc && cp src/context.d.ts dist/context.d.ts && node ./dist/standalone.js --nexus-artifacts --nexus-exit",
    "watch": "tsc -d -w --sourceMap"
  },
  "dependencies": {
    "@peeky/client-dist": "^0.13.6",
    "@peeky/config": "^0.13.6",
    "@peeky/runner": "^0.13.6",
    "@sefinek/random-emoji": "^1.4.3",
    "ansi_up": "^5.1.0",
    "apollo-server-core": "^3.10.2",
    "apollo-server-express": "^3.10.2",
    "chokidar": "^3.5.3",
    "consola": "^2.15.3",
    "express": "^4.18.1",
    "express-history-api-fallback": "^2.2.1",
    "fast-glob": "^3.2.12",
    "fs-extra": "^10.1.0",
    "graphql": "^16.6.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^5.11.2",
    "launch-editor": "^2.6.0",
    "nanoid": "^4.0.0",
    "nexus": "^1.3.0",
    "object-inspect": "^1.12.2",
    "pathe": "^0.3.8",
    "slugify": "^1.6.5",
    "vite": "^3.1.3",
    "ws": "^8.9.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.21",
    "@types/ws": "^8.2.2",
    "typescript": "^4.8.3"
  }
}
